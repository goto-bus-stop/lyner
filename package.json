    "url": "https://github.com/goto-bus-stop/lyner.git"
  },
  "dependencies": {
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "browserify": "^13.1.1"
